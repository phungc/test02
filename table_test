CREATE TABLE industry_tbl AS
SELECT
  CONCAT(country_tbl.country_name, '_', unique_month_tbl.month) AS id,
  unique_month_tbl.month,
  country_tbl.country_name,
  country_tbl.country_code,
  CASE MOD(unique_month_tbl.seqnum, 3)
    WHEN 0 THEN 'Finance'
    WHEN 1 THEN 'Technology'
    ELSE 'Retail'
  END AS industry
FROM
  (SELECT ROW_NUMBER() OVER (ORDER BY seq4()) AS seqnum, month FROM unique_month_tbl) unique_month_tbl
CROSS JOIN
  (SELECT country_name, country_code FROM country_tbl) country_tbl;
