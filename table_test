CREATE TABLE random_data_tbl AS
SELECT 
  ROW_NUMBER() OVER() AS row_id, 
  country_tbl.country_name, 
  country_tbl.country_code, 
  country_tbl.currency, 
  unique_month_tbl.month, 
  'industry_' || FLOOR(RAND()*(10-1+1)+1)::VARCHAR AS industry_name,
  SUBSTR(TO_HEX(RANDOM()), 1, 10) AS random_str_10_chars,
  CONCAT(SUBSTR(TO_HEX(RANDOM()), 1, 2), FLOOR(RAND()*(99999-10000+1)+10000)::VARCHAR) AS random_str_2_chars_5_nums,
  FLOOR(RAND()*(9999999999-1000000000+1)+1000000000)::VARCHAR AS random_str_10_nums
FROM country_tbl
CROSS JOIN unique_month_tbl
QUALIFY ROW_NUMBER() OVER(PARTITION BY country_tbl.country_code, unique_month_tbl.month ORDER BY RANDOM()) = 1
LIMIT 100;
