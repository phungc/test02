CREATE TABLE random_industry_tbl AS
SELECT
  CONCAT(country_tbl.country_name, '_', unique_month_tbl.month) AS id,
  unique_month_tbl.month,
  country_tbl.country_name,
  country_tbl.country_code,
  CASE MOD(unique_month_tbl.seqnum + country_tbl.seqnum, 3)
    WHEN 0 THEN 'Finance'
    WHEN 1 THEN 'Technology'
    ELSE 'Retail'
  END AS industry,
  RANDOMSTRING(10) AS random_string_10,
  CONCAT(RANDOMSTRING(2), RANDOMSTRING(5, '0123456789')) AS random_string_2_5,
  RANDOM() * 10000000000 AS random_number_10
FROM
  (SELECT month, ROW_NUMBER() OVER (ORDER BY RANDOM()) AS seqnum FROM unique_month_tbl) unique_month_tbl
CROSS JOIN
  (SELECT country_name, country_code, ROW_NUMBER() OVER (ORDER BY RANDOM()) AS seqnum FROM country_tbl) country_tbl
WHERE
  unique_month_tbl.seqnum <= 24
  AND country_tbl.seqnum <= 5
LIMIT
  100;
